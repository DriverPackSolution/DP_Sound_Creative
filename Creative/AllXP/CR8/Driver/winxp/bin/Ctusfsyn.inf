[WDM_SFSYNTH.AddReg]
HKR,,DevLoader,,*NTKERN
HKR,,NTMPDriver,,ctusfsyn.sys
HKLM,%StrCTKey%\SFSWEng\SoundFont,SoundFontBank,,"DEFAULT4.SFM"

[WDM_SFSYNTH.AddReg.NTAMD64]
HKR,,DevLoader,,*NTKERN
HKR,,NTMPDriver,,ctusfsyn.sys
