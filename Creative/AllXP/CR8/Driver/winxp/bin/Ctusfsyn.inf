[DestinationDirs]
WDM_SFSYNTH.CopyFiles = 10,system32\drivers ; %SystemRoot%\system32\drivers
WDM_SFSYNTH.CopyFiles.ntamd64 = 10,system32\drivers ; %SystemRoot%\system32\drivers		
